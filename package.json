{
    "name": "react-webpack-typescript-babel",
    "version": "1.0.0",
    "description": "A sample setup using React, Webpack and Babel to build TypeScript code, and using TypeScript for type-checking. Based https://github.com/a-tarasyuk/react-webpack-typescript-babel",
    "scripts": {
        "type-check": "tsc --noEmit",
        "type-check:watch": "npm run type-check -- --watch",
        "build": "NODE_ENV=production webpack --progress --color --mode production",
        "start": "NODE_ENV=development webpack-dev-server --color --info"
    },
    "author": {
        "name": "Ruslan G.",
        "email": "thejavek@gmail.com"
    },
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "7.11.4",
        "@babel/preset-env": "7.11.0",
        "@babel/preset-react": "7.10.4",
        "@babel/preset-typescript": "7.10.4",
        "@types/react": "^16.9.48",
        "@types/react-dom": "^16.9.8",
        "@types/webpack": "^4.41.21",
        "@types/webpack-dev-server": "^3.11.0",
        "babel-loader": "8.1.0",
        "fork-ts-checker-webpack-plugin": "5.1.0",
        "html-webpack-plugin": "4.3.0",
        "typescript": "^4.0.2",
        "webpack": "4.44.1",
        "webpack-cli": "3.3.12",
        "webpack-dev-server": "3.11.0"
    },
    "dependencies": {
        "core-js": "3",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "regenerator-runtime": "^0.13.7"
    }
}
